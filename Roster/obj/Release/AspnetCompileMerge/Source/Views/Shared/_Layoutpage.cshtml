
<head>
    <meta name="description" content="Vali is a responsive and free admin theme built with Bootstrap 4, SASS and PUG.js. It's fully customizable and modular.">
    <!-- Twitter meta-->
    <meta property="twitter:card" content="summary_large_image">

    <!-- Open Graph Meta-->
    <meta property="og:type" content="website">
    <meta property="og:site_name" content="Vali Admin">
    <meta property="og:title" content="Vali - Free Bootstrap 4 admin theme">
    <meta property="og:url" content="http://pratikborsadiya.in/blog/vali-admin">
    <meta property="og:image" content="http://pratikborsadiya.in/blog/vali-admin/hero-social.png">
    <meta property="og:description" content="Vali is a responsive and free admin theme built with Bootstrap 4, SASS and PUG.js. It's fully customizable and modular.">
    <title>Roster System</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Main CSS-->
    <link rel="stylesheet" type="text/css" href="~/css/main.css">
    <!-- Font-icon css-->
    <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
</head>
<body class="app sidebar-mini rtl">
    <!-- Navbar-->
    @using Microsoft.AspNet.Identity
    @if (Request.IsAuthenticated)
    {
        using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
        {
            @Html.AntiForgeryToken()
            <header class="app-header">
                <a class="app-header__logo"><img src="~/images/devcool-logo-h-white.png" style="padding: 15px 0px;" /></a>
                <!-- Sidebar toggle button--><a class="app-sidebar__toggle" href="#" data-toggle="sidebar" aria-label="Hide Sidebar" style="padding: 15px 10px;"></a>
                <!-- Navbar Right Menu-->

                <ul class="app-nav" style="padding: 15px 10px;">
                    <li class="dropdown">
                        @if (!string.IsNullOrEmpty(Session["UserName"].ToString()))
                        {
                            <a class="app-nav__item" href="#" data-toggle="dropdown" aria-label="Open Profile Menu">
                                @Session["UserName"]
                                <i class="fa fa-user fa-lg"></i>
                            </a>
                            <ul class="dropdown-menu settings-menu dropdown-menu-right">
                                <li><a class="dropdown-item" href="/Manage/AfterLoginChangePassword"><i class="fa fa-cog fa-lg"></i>Change Password</a></li>
                                <li><a class="dropdown-item" href="javascript:document.getElementById('logoutForm').submit()"><i class="fa fa-sign-out fa-lg"></i> Logout</a></li>
                            </ul>
                        }
                    </li>

                </ul>
              
            </header>
        }
    }
            <!-- Sidebar menu-->
            <div class="app-sidebar__overlay" data-toggle="sidebar"></div>

            <aside class="app-sidebar">
                @if (Convert.ToInt32(Session["MemberRole"]) == 1)
                {
                <ul class="app-menu">

                    <li class="treeview">
                        <a class="app-menu__item" href="/Account/RegisterUser">
                            <i class="app-menu__icon fa fa-laptop">

                            </i><span class="app-menu__label">User Management</span><i class="treeview-indicator fa fa-angle-right"></i>
                        </a>

                    </li>
                    <li class="treeview">
                        <a class="app-menu__item" href="/Employee/EmployeeDetails">
                            <i class="app-menu__icon fa fa-laptop">

                            </i><span class="app-menu__label">Create Employee</span><i class="treeview-indicator fa fa-angle-right"></i>
                        </a>

                    </li>
                    <li class="treeview">
                        <a class="app-menu__item" href="/Roster/RosterDetails">
                            <i class="app-menu__icon fa fa-laptop">

                            </i><span class="app-menu__label"> Create Employee Details </span><i class="treeview-indicator fa fa-angle-right"></i>
                        </a>

                    </li>
                    <li class="treeview">
                        <a class="app-menu__item" href="/Roster/Index">
                            <i class="app-menu__icon fa fa-laptop">

                            </i><span class="app-menu__label">Roster Details</span><i class="treeview-indicator fa fa-angle-right"></i>
                        </a>

                    </li>
                    <li class="treeview">
                        <a class="app-menu__item" href="/Report/EmployeeDetailbyCustomerReport">
                            <i class="app-menu__icon fa fa-laptop">
                            </i><span class="app-menu__label">Customer wise Details</span><i class="treeview-indicator fa fa-angle-right"></i>
                        </a>
                    </li>
                </ul>
                }
                else if (Convert.ToInt32(Session["MemberRole"]) == 2)
                {
                    <ul class="app-menu">


                        <li class="treeview">
                            <a class="app-menu__item" href="/Employee/EmployeeDetails">
                                <i class="app-menu__icon fa fa-laptop">

                                </i><span class="app-menu__label">Create Employee</span><i class="treeview-indicator fa fa-angle-right"></i>
                            </a>

                        </li>
                        <li class="treeview">
                            <a class="app-menu__item" href="/Roster/RosterDetails">
                                <i class="app-menu__icon fa fa-laptop">

                                </i><span class="app-menu__label"> Create Employee Details </span><i class="treeview-indicator fa fa-angle-right"></i>
                            </a>

                        </li>
                        <li class="treeview">
                            <a class="app-menu__item" href="/Roster/Index">
                                <i class="app-menu__icon fa fa-laptop">

                                </i><span class="app-menu__label">Roster Details</span><i class="treeview-indicator fa fa-angle-right"></i>
                            </a>

                        </li>
                    </ul>
                }
                else if (Convert.ToInt32(Session["MemberRole"]) == 3)
                {
                    <ul class="app-menu">
                        <li class="treeview">
                            <a class="app-menu__item" href="/Roster/Index">
                                <i class="app-menu__icon fa fa-laptop">

                                </i><span class="app-menu__label">Roster Details</span><i class="treeview-indicator fa fa-angle-right"></i>
                            </a>

                        </li>
                    </ul>
                }
            </aside>

            <!-- Essential javascripts for application to work-->
            <script src="~/js/jquery-3.2.1.min.js"></script>
            <script src="~/js/popper.min.js"></script>
            <script src="~/js/bootstrap.min.js"></script>
            <script src="~/js/main.js"></script>
            <!-- The javascript plugin to display page loading on top-->
            <script src="~/js/plugins/pace.min.js"></script>
            <!-- Page specific javascripts-->
            <script type="text/javascript" src="~/js/plugins/chart.js"></script>
